{
  "trailingComma": "es5",
  "semi": true,
  "singleQuote": true,
  "arrowParens": "always",
  "printWidth": 100,
  "tabWidth": 4,
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "^react$",
    "^next",
    "<THIRD_PARTY_MODULES>",
    "^@/",
    "interface",
    "(?=content|api)",
    "context/",
    "mock/",
    "config",
    "utils/",
    "hooks/",
    "(components/|./index)",
    "(.svg|.png|.jpg|.webp)",
    "^../(.*)$"
  ]
}